------------------------------------------------select piola
SELECT 
    pr.id_provincia AS "numero",
    pr.nombre AS "provincia",
    re.nombre
FROM provincias pr
    JOIN regiones re ON (re.id_region=pr.region_id)
    ORDER BY pr.id_provincia DESC
